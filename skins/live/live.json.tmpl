        "moonDeclination" : "$("%.1f&deg;" % $almanac.moon.dec)",

	       #if $almanac.next_full_moon.raw < $almanac.next_new_moon.raw
        "fullMoon" : "$almanac.next_full_moon",
        "newMoon" : "$almanac.next_new_moon",
        #else
        "fullMoon" : "$almanac.next_new_moon",
        "newMoon" : "$almanac.next_full_moon",
        #end if
        "moonPhase" : "$almanac.moon_phase ($almanac.moon_fullness% full)"
    #else
