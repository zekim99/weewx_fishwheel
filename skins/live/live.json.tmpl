  "almanac" : {
    #if $almanac.hasExtras
      ## Extended almanac information is available. Do the full set of tables.
      #set $sun_altitude = $almanac.sun.alt
      #if $sun_altitude < 0
        #set $sun_None="(Always down)"
      #else
        #set $sun_None="(Always up)"
      #end if
        "startCivilTwilight" : "$almanac(horizon=-6).sun(use_center=1).rise",
        "sunrise" : "$almanac.sun.rise.string($sun_None)",
        "transit" : "$almanac.sun.transit",
        "sunset" : "$almanac.sun.set.string($sun_None)",
        "endCivilTwilight" : "$almanac(horizon=-6).sun(use_center=1).set",
        "azimuth" : "$("%.1f&deg;" % $almanac.sun.az)",
        "altitude" : "$("%.1f&deg;" % $sun_altitude)",
        "rightAscension" : "$("%.1f&deg;" % $almanac.sun.ra)",
        "declination" : "$("%.1f&deg;" % $almanac.sun.dec)",
        #if $almanac.next_equinox.raw < $almanac.next_solstice.raw
        #typically, ui should display whichever is next. if equinox before solstace, display it before solstace. otherwise inverse
        "isEquinoxBeforeSolstace" : "true",
        #else
        "isEquinoxBeforeSolstace" : "false",
        #end if
        "equinox" : "$almanac.next_equinox",
        "solstace" : "$almanac.next_solstice",

        "moonrise" : "$almanac.moon.rise",
        "moonTransit" : "$almanac.moon.transit",
